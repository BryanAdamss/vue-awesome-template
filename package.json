{
  "name": "vue-awesome-template",
  "version": "0.0.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite --mode dev",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest --environment jsdom",
    "build": "pnpm build:prod",
    "build:prod": "vite build --mode prod",
    "build:staging": "vite build --mode staging",
    "build:analysis": "vite build --mode analysis",
    "check-build": "run-p type-check build:prod",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:raw": "eslint --print-config .eslintrc.js > eslintrc-raw.json",
    "fetch-def": "node .swagger-ts/index.mjs",
    "gen-req": "node .swagger-ts/gen-request.mjs",
    "swg2ts": "pnpm fetch-def && pnpm gen-req"
  },
  "dependencies": {
    "eventemitter3": "^4.0.7",
    "openapi-typescript-fetch": "^1.1.3",
    "pinia": "^2.0.14",
    "tailwindcss": "^3.1.8",
    "vue": "^3.2.37",
    "vue-router": "^4.0.16"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.2",
    "@pnpm/lockfile-file": "^5.1.4",
    "@pnpm/lockfile-utils": "^4.0.10",
    "@pnpm/lockfile-walker": "^5.0.10",
    "@pnpm/logger": "^4.0.0",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^16.11.41",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/test-utils": "^2.0.0",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.19.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-vue": "^9.0.0",
    "globby": "^13.1.2",
    "jsdom": "^20.0.0",
    "npm-run-all": "^4.1.5",
    "openapi-typescript": "^5.4.1",
    "postcss": "^8.4.14",
    "prettier": "^2.5.1",
    "rollup-plugin-visualizer": "^5.7.0",
    "ts-node": "^10.9.1",
    "typescript": "~4.7.4",
    "unplugin-auto-import": "^0.9.3",
    "vite": "^3.0.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html-env": "^1.2.2",
    "vite-plugin-inspect": "^0.6.0",
    "vite-plugin-use-modules": "^1.3.1",
    "vitest": "^0.15.1",
    "vue-tsc": "^0.38.1"
  },
  "engines": {
    "node": ">= 16.16.0",
    "npm": ">= 8.11.0",
    "pnpm": ">=7.5.0"
  }
}
